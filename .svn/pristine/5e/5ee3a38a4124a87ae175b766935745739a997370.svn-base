package com.vodafone.frt.models;

public class MsgData {
    private String receiverId;
    private String userId;
    private String message;
    private String mobileTime;

    public MsgData(String userChatId, String senderChatId, String text, String mobileTime) {
        this.userId = userChatId;
        this.receiverId = senderChatId;
        this.message = text;
        this.mobileTime = mobileTime;
    }

    public String getMobileTime() {
        return mobileTime;
    }

    public void setMobileTime(String mobileTime) {
        this.mobileTime = mobileTime;
    }

    public String getReceiverId() {
        return receiverId;
    }

    public void setReceiverId(String receiverId) {
        this.receiverId = receiverId;
    }

    public String getUserId() {
        return userId;
    }

    public void setUserId(String userId) {
        this.userId = userId;
    }

    public String getMessage() {
        return message;
    }

    public void setMessage(String message) {
        this.message = message;
    }
}
